<% price_target = XStocks::Charts::PriceTarget.new(@stock, details) %>
<% if price_target.data? %>
<div class="small-widget my-side-panel mx-auto mx-lg-0">
  <h4><%= label %></h4>
  <div style="display: block; width: 280px; height: 85px; margin-top: 15px;" class="mx-auto">
    <%= price_target.render(width: 280, height:85) %>
  </div>
</div>
<% end %>
